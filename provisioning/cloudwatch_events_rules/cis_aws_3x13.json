  "provisioning": {
    "eventSettings": {
      "eventName": "foundation-cis.aws.3.13-detect-route-table-changes",
      "eventDescription": "CIS AWS 3.13 - Detect route table change events",
      "eventPattern": {
        "detail-type": [
          "AWS API Call via CloudTrail"
        ],
        "detail": {
          "eventSource": [
            "ec2.amazonaws.com"
          ],
          "eventName": [
            "CreateRoute",
            "CreateRouteTable",
            "ReplaceRoute",
            "ReplaceRouteTableAssociation",
            "DeleteRouteTable",
            "DeleteRoute",
            "DisassociateRouteTable"
          ]
        }
      }
    }
  },
  "postDetectionEnrichment": {
    "customerSeverity": "Medium"
  },
  "auditing": {
    "lastAttestationDate": "20210808",
    "auditor": {
      "Name": "Donald Duck",
      "Email": "donald.duck@nuvibit.com"
    }
  }
