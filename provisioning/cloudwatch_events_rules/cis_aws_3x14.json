  "provisioning": {
    "eventSettings": {
      "eventName": "foundation-cis.aws.3.14-detect-vpc-changes",
      "eventDescription": "CIS AWS 3.14 - Detect VPC change events",
      "eventPattern": {
        "detail-type": [
          "AWS API Call via CloudTrail"
        ],
        "detail": {
          "eventSource": [
            "ec2.amazonaws.com"
          ],
          "eventName": [
            "CreateVpc",
            "DeleteVpc",
            "ModifyVpcAttribute",
            "AcceptVpcPeeringConnection",
            "CreateVpcPeeringConnection",
            "DeleteVpcPeeringConnection",
            "RejectVpcPeeringConnection",
            "AttachClassicLinkVpc",
            "DetachClassicLinkVpc",
            "DisableVpcClassicLink",
            "EnableVpcClassicLink"
          ]
        }
      }
    }
  },
  "postDetectionEnrichment": {
    "customerSeverity": "Medium"
  },
  "auditing": {
    "lastAttestationDate": "20210808",
    "auditor": {
      "Name": "Donald Duck",
      "Email": "donald.duck@nuvibit.com"
    }
  }
